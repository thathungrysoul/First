class Solution {
public:
    int mySqrt(int x) {
        int s=0,e=x,m;
        while(s<=e){
            m=s+(e-s)/2;
            if((long long)m*m==x)
                return m;
            else if((long long)m*m>x)
                e=m-1;
            else 
                s=m+1;
        }
        return e;
    }
};
